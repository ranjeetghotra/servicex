    "scripts": {
        "start": "npm-run-all --parallel start-backend start-frontend start-admin",
        "start-backend": "cd backend && npm start",
        "start-frontend": "cd frontend && npm start",
        "start-admin": "cd admin && npm start",
        "build": "npm-run-all --parallel build-backend build-frontend build-admin",
        "build-backend": "cd backend && npm run build",
        "build-frontend": "cd frontend && npm run build",
        "build-admin": "cd admin && npm run build",
        "install-all": "npm-run-all --parallel install-backend install-frontend install-admin",
        "install-backend": "cd backend && npm install",
        "install-frontend": "cd frontend && npm install",
        "install-admin": "cd admin && npm install"
